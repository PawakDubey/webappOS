<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>newStudentServlet</servlet-name>
		<display-name>StudentSubmit</display-name>
		<description></description>
		<servlet-class>com.openshift.test.controller.StudentSubmit</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>newStudentServlet</servlet-name>
		<url-pattern>/studentSubmit</url-pattern>
	</servlet-mapping>
	<listener>
		<listener-class>com.openshift.test.util.HibernateListener</listener-class>
	</listener>
</web-app>

